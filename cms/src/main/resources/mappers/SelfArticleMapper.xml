<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="top.roud.cms.mapper.SelfArticleMapper">
    <sql id="selfArticleField">
        id,articleId,validateCode
    </sql>
    <select id="selectSelfArticleByArticleId" resultType="SelfArticle">
        select
            <include refid="selfArticleField"/>
            from
              rb_selfarticle
            where
              articleId = #{articleId}
    </select>

</mapper>